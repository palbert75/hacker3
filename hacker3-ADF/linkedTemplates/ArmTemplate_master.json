{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hacker3-ADF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"CloudSalesDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSalesDatabase'"
		},
		"CloudStreaming_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreaming'"
		},
		"CosmosSouthMovies_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosSouthMovies'"
		},
		"FourthCoffeeRentals_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FourthCoffeeRentals'"
		},
		"VanArsdelSQLConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VanArsdelSQLConnection'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hacker3sta.dfs.core.windows.net"
		},
		"FourthCoffeeRentals_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"FourthCoffeeRentals_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "southridge"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hacker3-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"CloudSalesDatabase_connectionString": {
						"value": "[parameters('CloudSalesDatabase_connectionString')]"
					},
					"CloudStreaming_connectionString": {
						"value": "[parameters('CloudStreaming_connectionString')]"
					},
					"CosmosSouthMovies_connectionString": {
						"value": "[parameters('CosmosSouthMovies_connectionString')]"
					},
					"FourthCoffeeRentals_password": {
						"value": "[parameters('FourthCoffeeRentals_password')]"
					},
					"VanArsdelSQLConnection_connectionString": {
						"value": "[parameters('VanArsdelSQLConnection_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"FourthCoffeeRentals_properties_typeProperties_host": {
						"value": "[parameters('FourthCoffeeRentals_properties_typeProperties_host')]"
					},
					"FourthCoffeeRentals_properties_typeProperties_userId": {
						"value": "[parameters('FourthCoffeeRentals_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "hacker3-ADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'hacker3-ADF_ArmTemplate_0')]"
			]
		}
	]
}